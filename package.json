{
  "name": "gatsby-starter-typescript-contentful",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby with Typescript and Contentful",
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint-fix": "eslint --ext ts --fix ./src",
    "lint": "eslint --ext ts ./src",
    "clean": "gatsby clean && rm -rf public",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@contentful/gatsby-transformer-contentful-richtext": "^13.1.0",
    "@contentful/rich-text-react-renderer": "^14.1.2",
    "@contentful/rich-text-types": "^14.1.2",
    "@emotion/react": "^11.1.4",
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.31.1",
    "gatsby-image": "^2.10.0",
    "gatsby-plugin-emotion": "^5.3.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-sharp": "^2.13.1",
    "gatsby-plugin-sitemap": "^2.11.0",
    "gatsby-plugin-smoothscroll": "^1.2.0",
    "gatsby-plugin-ts-config": "^1.1.5",
    "gatsby-source-contentful": "^4.5.1",
    "gatsby-source-filesystem": "^2.10.0",
    "gatsby-transformer-sharp": "^2.11.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "twin.macro": "^2.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ersanyamarya/gatsby-typescript-tailwindcss-contentful-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/ersanyamarya/gatsby-typescript-tailwindcss-contentful-boilerplate/issues"
  },
  "homepage": "https://github.com/ersanyamarya/gatsby-typescript-tailwindcss-contentful-boilerplate#readme",
  "author": {
    "name": "Sanyam Arya",
    "email": "er.sanyam.arya@gmail.com",
    "url": "https://github.com/ersanyamarya"
  }
}

{
  "name": "gatsby-starter-typescript-contentful",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby with Typescript and Contentful",
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint-fix": "eslint --ext ts --fix ./src",
    "lint": "eslint --ext ts ./src",
    "clean": "gatsby clean && rm -rf public",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@contentful/gatsby-transformer-contentful-richtext": "^13.1.0",
    "@contentful/rich-text-react-renderer": "^14.1.3",
    "@contentful/rich-text-types": "^14.1.2",
    "@emailjs/browser": "^3.4.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/utils": "^4.11.2",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.2",
    "axios": "^0.26.0",
    "dotenv": "^8.6.0",
    "emailjs-com": "^3.2.0",
    "env-cmd": "^10.1.0",
    "fs": "^0.0.1-security",
    "gatsby": "^2.32.12",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-config": "^1.0.7",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.3",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-smoothscroll": "^1.2.0",
    "gatsby-plugin-ts-config": "^1.1.5",
    "gatsby-source-contentful": "^4.6.4",
    "gatsby-source-custom-api": "^2.3.2",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-sharp": "^2.12.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.49.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@graphql-codegen/typescript-react-apollo": "^2.2.3",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ersanyamarya/gatsby-typescript-materialui-contentful-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/ersanyamarya/gatsby-typescript-materialui-contentful-boilerplate/issues"
  },
  "homepage": "https://github.com/ersanyamarya/gatsby-typescript-materialui-contentful-boilerplate#readme",
  "author": {
    "name": "Sanyam Arya",
    "email": "er.sanyam.arya@gmail.com",
    "url": "https://github.com/ersanyamarya"
  }
